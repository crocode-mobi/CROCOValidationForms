FRAMEWORK_SEARCH_PATHS = $(inherited) "$(SDKROOT)/Developer/Library/Frameworks" "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CROCOValidationForms" "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CROCOValidationForms" -isystem "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase"
OTHER_LDFLAGS = -ObjC -l"Pods-CROCOValidationForms_Tests-CROCOValidationForms" -l"Pods-CROCOValidationForms_Tests-FBSnapshotTestCase" -framework "Foundation" -framework "XCTest"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods